<view class="tui-userinfo-box"><tui-list-cell vue-id="6126fc01-1" padding="0" hover="{{false}}" bind:__l="__l" vue-slots="{{['default']}}"><view class="tui-list-cell"><view>头像</view><image class="tui-avatar" src="{{userInfo.avatarUrl}}"></image></view></tui-list-cell><tui-list-cell vue-id="6126fc01-2" padding="0" arrow="{{true}}" data-event-opts="{{[['^tap',[['editname']]]]}}" bind:tap="__e" bind:__l="__l" vue-slots="{{['default']}}"><view class="tui-list-cell"><view>用户昵称</view><view class="tui-content">{{userInfo.nickName}}</view></view></tui-list-cell><tui-list-cell vue-id="6126fc01-3" padding="0" hover="{{false}}" bind:__l="__l" vue-slots="{{['default']}}"><view class="tui-list-cell"><view>性别</view><view class="tui-content">{{['男','女'][userInfo.gender]}}</view></view></tui-list-cell><picker mode="date" data-event-opts="{{[['change',[['changeDate',['$event']]]]]}}" bindchange="__e"><tui-list-cell vue-id="6126fc01-4" padding="0" arrow="{{true}}" unlined="{{true}}" bind:__l="__l" vue-slots="{{['default']}}"><view class="tui-list-cell"><view>出生日期</view><view class="tui-content">{{userInfo.birthDay}}</view></view></tui-list-cell></picker></view>