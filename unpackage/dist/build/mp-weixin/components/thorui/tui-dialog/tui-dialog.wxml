<view><block wx:if="{{show}}"><view data-event-opts="{{[['tap',[['stopEvent',['$event']]]]]}}" class="tui-dialog" style="{{'background:'+(backgroundColor)+';'+('border-radius:'+(radius)+';')}}" catchtap="__e"><view class="tui-dialog__hd"><view class="tui-dialog__title" style="{{'color:'+(titleColor)+';'}}">{{title+''}}<slot name="title"></slot></view></view><view class="tui-dialog__bd" style="{{'color:'+(contentColor)+';'}}"><slot name="content"></slot></view><view class="tui-dialog__ft"><block wx:if="{{buttons&&buttons.length}}"><block><block wx:for="{{buttons}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="tui-dialog__btn" style="{{'color:'+(item.color||'#333')+';'}}" data-index="{{index}}" data-event-opts="{{[['tap',[['buttonTap',['$event']]]]]}}" bindtap="__e">{{item.text}}</view></block></block></block><block wx:else><slot name="footer"></slot></block></view></view></block><block wx:if="{{mask}}"><view data-event-opts="{{[['tap',[['close',['$event']]]]]}}" class="{{['tui-dialog__mask',(!show)?'tui-mask_hidden':'']}}" bindtap="__e"></view></block></view>